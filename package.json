{
  "devDependencies": {
    "cypress": "^14.0.1",
    "dotenv": "^16.4.7",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "scripts": {
    "cypress:runhw17": "npx cypress run --spec cypress/e2e/hw17.cy.js",
    "cypress:runhw18": "npx cypress run --spec cypress/e2e/hw18.cy.js",
    "cypress:runhw20": "npx cypress run --spec cypress/e2e/hw20.cy.js --env VERSION=NORM --reporter mochawesome",
    "cypress:runhw21": "npx cypress run --spec cypress/e2e/hw21.cy.js --env VERSION=NORM",
    "test:hw20norm": "npx cypress open --env VERSION=NORM",
    "test:hw20bug": "npx cypress open --env VERSION=BUG"
  },
  "dependencies": {
    "cypress-real-events": "^1.14.0"
  }
}
